<UserControl x:Class="Filtration.Views.ToolPanes.DisplayWindowView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:converters="clr-namespace:Filtration.Converters"
             xmlns:toolPanes="clr-namespace:Filtration.ViewModels.ToolPanes"
             d:DataContext="{d:DesignInstance Type=toolPanes:DisplayWindowViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.Resources>
        <converters:HashSignRemovalConverter x:Key="HashSignRemovalConverter" />
    </UserControl.Resources>
</UserControl>
